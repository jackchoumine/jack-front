{
  "liveSassCompile.settings.formats": [
    {
      // nested 嵌套模式
      // compressed 压缩模式
      // compact 紧凑模式
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "~/css"
    }
  ],
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "compile-hero.disable-compile-files-on-did-save-code": false,
  "livePreview.defaultPreviewPath": "/packages/learn-scss/index.html",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "yaml": false
  }
}
